{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": true,
    //enable sourceMap to get .js.map files
    "sourceMap": false,
    "noEmitOnError": true,
    "strictNullChecks": true,
    "noUnusedParameters": true
  },
  "exclude": [
      "node_modules",
      "**/junk/*.ts"
  ]
}
